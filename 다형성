import 'dart:math';

import 'package:flutter/material.dart';

void main() {
  runApp(MaterialApp(
    home: Main(),
    )
  );
}
class Main extends StatelessWidget {
  const Main({super.key});

  @override
  Widget build(BuildContext context) {
    final r = Random();
    String gender = r.nextDouble() > 0.5 ? '남자' : '여자';
    print(gender);

    return Scaffold(
      body: Center(
        child: getWidget(gender)
      ),
    );
  }

  Widget getWidget(String g) {
    return g =='남자' ? FilledButton(onPressed: () {}, child: Text(g))
        : ElevatedButton(onPressed: () {}, child: Text(g));
  }

}
